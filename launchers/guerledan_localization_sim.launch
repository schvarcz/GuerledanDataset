<launch>
    <node pkg="helius" type="state_node" name="state_node" output="screen">
        <param name="log_file" value="/home/schvarcz/Dropbox/Doutorado-ENSTA/Guerledan/2017/BUBBLE_WORKSPACE/log/logstate_2017-10-13_10h59min56s_limpo.csv"/>
        <param name="time_diff" value="0"/>
        <param name="first_time" value="0"/>
    </node>

    <node pkg="helius" type="map_node" name="map_node" output="screen">
        <param name="log_file" value="/home/schvarcz/Dropbox/Doutorado-ENSTA/Guerlédan_2013_06/MRIS_XYZ_Guerledan_Retenue_2013_06_L93_IGN69_1x1.pts"/>
        <param name="minX" value="253150"/>
        <param name="maxX" value="253600"/>
        <param name="minY" value="6805440"/>
        <param name="maxY" value="6805940"/>
    </node>

    <node pkg="helius" type="simulated_readings_node" name="simulated_readings_node" output="screen">
        <param name="log_file" value="/home/schvarcz/Dropbox/Doutorado-ENSTA/Guerlédan_2013_06/MRIS_XYZ_Guerledan_Retenue_2013_06_L93_IGN69_1x1.pts"/>
        <param name="frame_id" value="base_blue_view"/>
        <param name="pixel_size" value="1"/>
        <param name="line_size" value="50"/>
        <param name="minX" value="253150"/>
        <param name="maxX" value="253600"/>
        <param name="minY" value="6805440"/>
        <param name="maxY" value="6805940"/>
        <!--<param name="minZ" value="6805440"/>-->
        <!--<param name="maxZ" value="6805940"/>-->
    </node>

    <node pkg="intervalar_localization" type="interval_localization_3D" name="interval_localization_3D" output="screen" >
            <!-- <remap from="/odom" to="/odom_encoder" /> -->
            <param name="localizeFPS" value="10" />
            <param name="useNreadings" value="40" />
            <param name="thresholdScan" value="0.15" />
            <param name="thresholdOdomOmega" value="0.5" />
            <param name="initialPoseX" value="253539.127054" />
            <param name="initialPoseY" value="6805743.6782" />
            <param name="initialPoseZ" value="122." />
            <param name="initialPoseTH" value="-0.535816" /> <!-- 0.4886922, 2.0717058-->
            <param name="uncertaintyXY" value="30." />
            <param name="uncertaintyZ" value="5." />
            <param name="uncertaintyTH" value="0.1672665" />
            <param name="vel_bounding_lb" value="1." />
            <param name="vel_bounding_ub" value="1." />
            <param name="odom_yaw_bounding_lb" value="1." />
            <param name="odom_yaw_bounding_ub" value="1." />
            <!-- <param name="laserscan_uncertainty" value="0.03" /> -->
    </node>
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find helius)/config/config.rviz" />
    <node pkg="tf" type="static_transform_publisher" name="odom2map" args="253539.127054 6805743.6782 0 0 0 0  map odom 1000" />
    <node pkg="tf" type="static_transform_publisher" name="bv2base_link" args="0 0 0 0 1.5708 0  base_link base_blue_view 1000" />
</launch>
